<UserControl x:Class="TextMessageExtractor.GUI.ConversationViewer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TextMessageExtractor.GUI"
             mc:Ignorable="d" 
             d:DesignHeight="640" d:DesignWidth="360">
    <Grid Background="Black">
        <ScrollViewer x:Name="scrollViewer" VerticalScrollBarVisibility="Hidden" Foreground="{x:Null}">
            <VirtualizingStackPanel x:Name="stackPanel">
                
            </VirtualizingStackPanel>
        </ScrollViewer>
    </Grid>
</UserControl>
